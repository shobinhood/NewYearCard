.social-link {
    display:block;
    height: 50px;
    width: 50px;
    float:left;
    -webkit-animation: social .6s steps(12,end) infinite;
    animation: social .6s steps(12,end) infinite;
    /*-webkit-animation-play-state: paused;
    animation-play-state: paused;*/
}
.social-twitter {
    background-image: url(http://www.ccareydownload.com/card_images/twitter-sprite.png)
}
.social-rss {
    background-image: url(http://www.ccareydownload.com/card_images/linkedin-sprite.png);
      -webkit-animation: social-rss .6s steps(9,end) infinite;
    animation: social-rss .6s steps(9,end) infinite;
      /*-webkit-animation-play-state: paused;
    animation-play-state: paused;*/
}
.social-facebook {
    background-image: url(http://www.ccareydownload.com/card_images/facebook-sprite.png);
}
@keyframes social {
    from { background-position: 0 0; }
    to { background-position: -600px 0; }
}
@-webkit-keyframes social {
    from { social-position: 0 0; }
    to { background-position: -600px 0; }
}
@keyframes social-rss {
    from { background-position: 0 0; }
    to { background-position: -450px 0; }
}
@-webkit-keyframes social-rss {
    from { social-position: 0 0; }
    to { background-position: -450px 0; }
	

}



h1 {
	font-family: "Kaushan Script";
	color: #fff;
	
	font-size: 70px;
	text-align: center;
	
	padding: 0px 0px 30px 0px;
	margin: 0px;
	opacity: 0;
	-webkit-animation: 1.5s linear 6s fadein forwards; /* Safari, Chrome and Opera > 12.1 */
            animation: 1.5s linear 6s fadein forwards;
}

.year {
	
	padding-left: 30%;
	width:120%;
	opacity: 0;
	-webkit-animation: 0.25s linear 7s fadein forwards , 3s appear forwards; /* Safari, Chrome and Opera > 12.1 */
			animation: 0.25s linear 7s fadein forwards , 3s appear forwards;
			
}

.arrow {
	position: absolute;
	width: 100%;
	padding-left: 36%;
	top: -40px;
	opacity: 0;
	-webkit-animation: 0.25s linear 7.8s fadein forwards;/* Safari, Chrome and Opera > 12.1 */
			animation: 0.25s linear 7.8s fadein forwards;
}

@keyframes fadein {
    from { opacity: 0; }
    to   { opacity: 1; }
}

@-webkit-keyframes fadein {
    from { opacity: 0; }
    to   { opacity: 1; }
}

@keyframes appear {
  0% {
    opacity: 0;
    transform: scale(0.0001);
  }
  
  100% {
    opacity: 1;
    transform: scale(1);
  }
}






